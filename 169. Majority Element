class Solution {
    public int majorityElement(int[] nums) {
        int current_num = 0;
        int count = 0;
        for(int i=0;i<nums.length;i++){
            if(count==0){
                current_num = nums[i];
                count++;
            }
            else if(current_num != nums[i]){
                count--;
            }
            else{
                count++;
            }
        }
        return current_num;
    }
}

class Solution {
    public int majorityElement(int[] nums) {
        HashMap<Integer, Integer> map1 = new HashMap<>();
        for(int i=0;i<nums.length;i++){
            map1.put(nums[i], map1.getOrDefault(nums[i],0)+1);
        }
        int max_len = 0;
        int major_element = 0;
        for(int key : map1.keySet()){
            int frequency = map1.get(key);
            if(max_len < frequency){
                max_len = frequency;
                major_element = key;
            }
        }

        return major_element;
    }
}

